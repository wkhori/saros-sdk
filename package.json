{
  "name": "@saros-finance/amm-sdk",
  "version": "3.0.0",
  "type": "module",
  "description": "Saros AMM - the engine that powers the Internet Capital Market on Solana.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "start": "tsup --watch",
    "prepare": "pnpm run build",
    "lint": "eslint . --ext .js,.ts --fix",
    "lint:check": "eslint . --ext .js,.ts",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "fix": "prettier --write . && eslint . --ext .ts --fix",
    "test": "vitest",
    "test:cleanup": "vitest run src/tests/integration/batch-close-positions.test.ts --reporter=verbose"
  },
  "author": "Saros Finance",
  "license": "ISC",
  "keywords": [
    "saros",
    "swap",
    "solana",
    "defi",
    "liquidity",
    "amm",
    "crypto",
    "farm",
    "stake",
    "finance"
  ],
  "dependencies": {
    "@coral-xyz/anchor": "0.31.1",
    "@solana/spl-token": "0.4.13",
    "@solana/web3.js": "1.98.4",
    "bs58": "^6.0.0",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.20",
    "@types/node": "^22.18.3",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "@vitest/ui": "^4.0.8",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.6.2",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "vite": "^7.2.2",
    "vitest": "^4.0.8"
  }
}
